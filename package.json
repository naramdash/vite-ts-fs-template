{
  "name": "vite-fs-ts",
  "version": "0.1.0",
  "scripts": {
    "postinstall": "dotnet tool restore",
    "format": "dotnet fantomas . --recurse",
    "trashfsjs": "trash ./src/**/*.fs.js",
    "dev": "dotnet fable watch src --run vite",
    "build": "tsc && dotnet fable src --run vite build",
    "serve": "vite preview"
  },
  "devDependencies": {
    "trash-cli": "^4.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.5.4"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  }
}
